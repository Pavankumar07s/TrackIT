{
  "expo": {
    "name": "tractit",
    "slug": "tractit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/TrackIt.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pavankumar-07s.tractit"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/TrackIt.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pavankumar_07s.tractit"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/TrackIt.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "13d8d8d6-47bd-4c9e-a469-a0723b6c4826"
      }
    },
    "owner": "pavankumar_07s"
  }
}
